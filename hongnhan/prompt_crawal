    },
    "b2c_audience": {
      "demographics": {
        "age_range": "18-24 | 25-34 | 35-44 | 45-54 | 55+",
        "gender": "Nam | Nữ | Tất cả",
        "income_level": "< 10M | 10-20M | 20-50M | > 50M VND/tháng",
        "education": "THPT | Đại học | Sau đại học",
        "marital_status": "Độc thân | Đã kết hôn | Có con nhỏ | Có con lớn",
        "occupation": "Ngành nghề phổ biến"
      },
      "psychographics": {
        "interests": ["Sở thích 1", "Sở thích 2"],
