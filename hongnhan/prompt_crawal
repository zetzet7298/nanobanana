        "values": ["Giá trị 1", "Giá trị 2"],
        "pain_points": ["Vấn đề/nỗi đau 1", "Vấn đề/nỗi đau 2"],
        "motivations": ["Động lực mua 1", "Động lực mua 2"],
        "objections": ["Lý do không mua 1", "Lý do không mua 2"]
      },
      "behaviors": {
        "online_behavior": "Heavy user > 4h/ngày | Moderate 2-4h | Light < 2h",
        "purchase_frequency": "Hàng ngày | Hàng tuần | Hàng tháng | Theo mùa",
